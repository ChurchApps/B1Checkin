{
  "expo": {
    "name": "B1Checkin",
    "slug": "B1Checkin",
    "version": "1.30.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "b1checkin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "churchapps",
    "extra": {
      "eas": {
        "projectId": "3e543eda-7610-49dc-b345-38234d7b9059"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "church.b1.checkin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
